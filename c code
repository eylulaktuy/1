#include<stdio.h>
void func(int num[],int x)
{
        int key,i,n,count=0,k;
	for(i=0;i<x;i++)
	{
		scanf("%d",&num[i]);
	}
	printf("\nEnter number to be deleted:");
	scanf("%d",&key);
	for(i=0;i<x;i++)
	{
		k=key;
		if(num[i]==k)
	    {
	    	n=i; 
	    	count++;
	    	if(n<=x+1)
			{
	    	    for(i=n;i<=x-count;i++)
	            {
		          num[i]=num[i+1];
	            }
	        }
	    }
	}
	printf("\nResulting array:");
	for(i=0;i<x-count;i++)
	{
	  	printf("%d ",num[i]);
	}
}
int main()
{
    int num[30];	
    int x;
    printf("Enter N:");
    scanf("%d",&x);
    printf("\nEnter %d integers:",x);
    func(num,x);
    return(0);
}
